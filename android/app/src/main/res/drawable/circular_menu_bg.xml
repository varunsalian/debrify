<?xml version="1.0" encoding="utf-8"?>
<!-- Circular menu container background -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <gradient
        android:angle="270"
        android:startColor="#70000000"
        android:centerColor="#60000000"
        android:endColor="#50000000"
        android:type="radial"
        android:gradientRadius="190" />
    <stroke
        android:width="1dp"
        android:color="#40FFFFFF" />
</shape>
